    "module": "es6",
    "sourceMap": true,
	"allowJs": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "rootDir": "./",
	"outDir": "./build",
    "lib": [
      "es2015",
      "dom"
    ],
    "baseUrl": "./",
    "paths": {
      "mobile-robotics-map-tools": ["declarations/mobile-robotics-map-tools.d.ts"],  // override definition for moment
      "protobuf": ["declarations/protobuf-index.d.ts"]  // override definition for moment
